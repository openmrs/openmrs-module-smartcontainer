<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.smartcontainer">
	 <class name="SMARTAppUser" table="smartcontainer_user">
	<id name="sMARTAppUserId" type="java.lang.Integer" column="smartcontainer_user_id" unsaved-value="0">
	<generator class="native"/>
	
	</id>
		  <set name="apps" table="smartcontainer_user_app" 
        	inverse="false" lazy="true" fetch="select" cascade="all" >
            <key>
                <column name="smartcontainer_user_id" not-null="true" />
            </key>
            <many-to-many entity-name="org.openmrs.module.smartcontainer.SMARTAppUser">
                <column name="smartcontainer_app_id" not-null="true" />
            </many-to-many>
        </set>
		<many-to-one name="openMRSUser" class="org.openmrs.User" column="user_id" not-null="true" cascade="all" unique="true" />
	</class>  
</hibernate-mapping>
