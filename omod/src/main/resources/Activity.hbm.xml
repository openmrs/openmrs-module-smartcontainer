<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.smartcontainer.app">
	<class name="Activity" table="smartcontainer_activity">
		<id name="activityId" type="java.lang.Integer" column="smartcontainer_activity_id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="activityName" type="java.lang.String" column="activity_name" />
		<property name="activityURL" type="java.lang.String" column="activity_url" />
	</class>
</hibernate-mapping>
